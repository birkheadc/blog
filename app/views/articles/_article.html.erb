<article id="<%= dom_id article %>">
  <div class="article-head">
    <h2><%= article.title %></h2>
    <hr>
    <h3><%= article.sub_title %></h3>
    <p class="posted-date">Posted: <%= article.created.strftime("%Y/%m/%d") %></p>
  </div>
  <p class="article-content"  ><%= article.content %></p>

</article>
